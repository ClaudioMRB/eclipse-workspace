package Projeto1;

import java.util.Scanner;

public class Poupanca{
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		System.out.println("digite numero da sua Conta: \n");
		String cc;
		//nome = nome;
		cc = scan.next();
		System.out.println("Nome \n" + cc);
		
	}
}
	